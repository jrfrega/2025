N = 200
df = data.frame(
casado = rbinom(N, 1, 0.7),
filhos = rbinom(N, 4, 0.2),
idade = round(rnorm(N, 40, 10), 0),
renda = rexp(N, 1/2000) %>% round(0) + 1000,
casapropria = rbinom(N, 1, 0.2)
)
apply(df, 2, scale) -> df
df1 = data.frame(1, df)
apply(df1, 2, as.numeric) -> df1
f = df1 %*% c(-0, -1, .5, -2.3, 1.2, -2.0)
y = (1/(1+exp(-f))+rnorm(N, 0, 0.2)) %>% round(0)
df = data.frame(y, df)
m = glm(y~., family = "binomial", data = df)
summary(m)
